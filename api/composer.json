{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "api-platform/api-pack": "^1.1",
        "doctrine/annotations": "^1.6",
        "dompdf/dompdf": "^0.8.3",
        "guzzlehttp/guzzle": "^6.3",
        "lexik/jwt-authentication-bundle": "^2.6",
        "nelmio/cors-bundle": "2.0.1",
        "oneup/flysystem-bundle": "^3.4",
        "sensio/framework-extra-bundle": "5.5.3",
        "stripe/stripe-php": "^7.3",
        "symfony/config": "^5.0",
        "symfony/console": "^5.0",
        "symfony/doctrine-bridge": "^5.0",
        "symfony/dotenv": "^5.0",
        "symfony/error-handler": "^5.0",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "^5.0",
        "symfony/http-foundation": "5.0.7",
        "symfony/http-kernel": "^5.0",
        "symfony/mercure-bundle": "^0.2",
        "symfony/monolog-bundle": "^3.4",
        "symfony/orm-pack": "^1.0",
        "symfony/security-core": "5.0.7",
        "symfony/security-http": "5.0.7",
        "symfony/swiftmailer-bundle": "3.4.0",
        "symfony/yaml": "^5.0",
        "vich/uploader-bundle": "^1.10"
    },
    "require-dev": {
        "api-platform/schema-generator": "^2.1",
        "doctrine/doctrine-fixtures-bundle": "3.3.0",
        "fzaninotto/faker": "^1.8",
        "roave/security-advisories": "dev-master",
        "symfony/debug-pack": "^1.0",
        "symfony/maker-bundle": "^1.11",
        "symfony/profiler-pack": "^1.0",
        "symfony/var-dumper": "^5.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "secure-http": false
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear -vvv": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^5.0"
        }
    }
}
